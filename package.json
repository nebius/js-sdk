{
  "name": "@nebius/nodejs-sdk",
  "version": "0.1.0",
  "description": "Nebius AI SDK for NodeJS",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./api/*": {
      "types": "./dist/esm/api/*.d.ts",
      "import": "./dist/esm/api/*.js",
      "require": "./dist/cjs/api/*.js"
    },
    "./runtime/*": {
      "types": "./dist/esm/runtime/*.d.ts",
      "import": "./dist/esm/runtime/*.js",
      "require": "./dist/cjs/runtime/*.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nebius/nodejs-sdk.git"
  },
  "keywords": [
    "nebius",
    "ai",
    "sdk",
    "typescript",
    "grpc",
    "protobuf"
  ],
  "author": "Nebius",
  "license": "Apache-2.0",
  "scripts": {
    "bootstrap:generator": "node scripts/bootstrap-generator.js",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:scripts": "tsc -p tsconfig.scripts.json && chmod +x dist-scripts/scripts/protoc-gen-nebius-ts-sdk.js dist-scripts/scripts/protoc-gen-nebius-ts.js",
    "build": "rimraf dist && npm run generate && npm run build:cjs && node scripts/build-esm-facades.js && node scripts/postbuild-mark-module-type.js",
    "clean:generated": "rimraf src/version.ts src/api",
    "clean": "npm run clean:generated && rimraf dist dist-scripts src/generated_test generated_test.lock",
    "docs": "typedoc",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "generate:full": "npm run generate && npm run bootstrap:generator",
    "generate:version": "node dist-scripts/scripts/version-gen.js",
    "generate": "npm run build:scripts && npm run clean:generated && buf generate nebius-api && npm run generate:version",
    "indexgen": "node -r ts-node/register scripts/gen-indexes.ts",
    "lint:fix": "eslint src/**/*.ts --fix --no-warn-ignored && npm run format",
    "lint": "eslint src/**/*.ts --no-warn-ignored",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "test:builds": "jest --testPathPatterns='build_'",
    "test:e2e": "jest --testPathPatterns='e2e_'",
    "test:noserver": "jest --testPathIgnorePatterns='e2e_|build_'",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.8.0",
    "dayjs": "^1.11.13",
    "fs-ext": "^2.1.1",
    "google-protobuf": "^3.21.4",
    "long": "^5.3.2",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.56.0",
    "@types/fs-ext": "^2.0.0",
    "@types/google-protobuf": "^3.15.12",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-tsdoc": "^0.4.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "lint-staged": "^16.2.0",
    "prettier": "^3.3.3",
    "protoc-gen-ts": "^0.8.7",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-proto": "^2.7.7",
    "typedoc": "^0.28.13",
    "typedoc-plugin-markdown": "^4.8.1",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix --no-warn-ignored",
      "prettier --write"
    ],
    "*.{json,md,yaml,yml}": [
      "prettier --write"
    ]
  },
  "overrides": {
    "test-exclude": "^7.0.1"
  }
}
