{
  "name": "@nebius/tssdk",
  "version": "0.1.0",
  "description": "Nebius AI SDK for TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/nebius/tssdk.git"
  },
  "keywords": [
    "nebius",
    "ai",
    "sdk",
    "typescript",
    "grpc",
    "protobuf"
  ],
  "author": "Nebius",
  "license": "Apache-2.0",
  "scripts": {
    "build:scripts": "tsc -p tsconfig.scripts.json && chmod +x dist-scripts/protoc-gen-nebius-ts-sdk.js",
    "build": "npm run generate && tsc",
    "generate": "rimraf src/generated && npm run build:scripts && buf generate nebius-api",
    "indexgen": "node -r ts-node/register scripts/gen-indexes.ts",
    "test": "jest --maxWorkers=8",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist src/generated",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.8.0",
    "google-protobuf": "^3.21.4",
    "long": "^5.3.2",
    "yaml": "^2.8.1",
    "fs-ext": "^2.1.1"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.56.0",
    "@types/google-protobuf": "^3.15.12",
    "@types/jest": "^30.0.0",
    "@types/fs-ext": "^2.0.0",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.33.0",
    "jest": "^30.0.5",
    "protoc-gen-ts": "^0.8.7",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.1",
    "ts-proto": "^2.7.7",
    "typescript": "^5.9.2",
    "ts-node": "^10.9.2",
    "ts-proto-descriptors": "^2.0.0"
  }
}
